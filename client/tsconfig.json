{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "target": "ES2020", 
    "module": "ESNext",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "expo-auth-session": ["node_modules/expo-auth-session"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}



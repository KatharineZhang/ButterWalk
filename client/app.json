
{
  "expo": {
    "name": "Husky SafeTrip",
    "slug": "butterwalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/butterWalkLogo.png",
    "scheme": "com.butterwalk.butterwalk",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/butterWalkLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.butterwalk.butterwalk",
      "config": {
        "googleMapsApiKey": "see app.config.ts"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app uses your location to match you with nearby rides.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to match you with nearby rides even in the background."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/butterWalkLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.butterwalk.butterwalk",
      "config": {
        "googleMaps": {
          "apiKey": "see app.config.ts"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/butterWalkLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-maps",
        {
          "iosGoogleMapsApiKey": "see app.config.ts",
          "androidGoogleMapsApiKey": "see app.config.ts"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "developmentClient": {},
    "extra": {
      "router": {
        "origin": false
      },
      "scheme": "butterWalk",
      "eas": {
        "projectId": "4091719b-1abc-4b53-ae61-14c967caffac"
      }
    }
  }
}

      },
      "package": "com.betterwalk.betterwalk",
      "config": {
        "googleMaps": {
        "apiKey": "AIzaSyChTTcBidR0I2xpN4COBPXddUCKv4E24R4"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/betterWalkLogo.png"

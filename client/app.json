        "googleMapsApiKey": "AIzaSyChTTcBidR0I2xpN4COBPXddUCKv4E24R4"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/betterWalkLogo.png",
